{
    "name":"fritzmg/contao-backend-documentation",
    "description":"Contao extension to show a simple back end documentation page generated from markdown.",
    "keywords":["contao", "news", "list"],
    "type":"contao-module",
    "homepage":"http://www.inspiredminds.at",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"Fritz Michael Gschwantner",
            "homepage":"http://www.inspiredminds.at",
            "email":"fmg@inspiredminds.at",
            "role":"Developer"
        }
    ],
    "support":{
        "email":"fmg@inspiredminds.at",
        "issues":"https://github.com/fritzmg/contao-backend-documentation/issues",
        "source":"https://github.com/fritzmg/contao-backend-documentation",
        "forum":"https://community.contao.org/de"
    },
    "require":{
        "php":">=5.4",
        "contao/core-bundle":"^3.0 || ^4.0 || ^5.0",
        "contao-community-alliance/composer-plugin":"~2.4 || ~3.0",
        "erusev/parsedown": "^1.6"
    },
    "extra":{
        "contao":{
            "sources": {
                "system/modules/backend_documentation":"system/modules/backend_documentation"
            }
        }
    },
    "autoload": {
        "classmap": [
            "system/modules/backend_documentation/modules"
        ]
    }
}
