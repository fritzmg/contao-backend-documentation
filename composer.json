{
    "name":"fritzmg/contao-backend-documentation",
    "description":"Contao extension to show a simple back end documentation page generated from markdown.",
    "keywords":["contao", "news", "list"],
    "type":"contao-module",
    "homepage":"http://www.inspiredminds.at",
    "license":"LGPL-3.0-or-later",
    "authors": [
        {
            "name": "INSPIRED MINDS",
            "homepage": "https://www.inspiredminds.at"
        }
    ],
    "support": {
        "issues": "https://github.com/inspiredminds/contao-personio/issues",
        "source": "https://github.com/inspiredminds/contao-personio"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=8.2",
        "contao/core-bundle": "^4.13 || ^5.0",
        "erusev/parsedown": "^1.6",
        "symfony/http-kernel": "^5.4 || ^6.4 || ^7.3"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoBackendDocumentation\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoBackendDocumentation\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^6.0",
        "contao/manager-plugin": "^2.12",
        "contao/rector": "^1.0",
        "maglnet/composer-require-checker": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "contao-community-alliance/composer-plugin": true,
            "contao/manager-plugin": true
        }
    }
}
